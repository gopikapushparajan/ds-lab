#include<stdio.h>
struct student{
    int usn;
    char name[10];
    int mobileno;
}s1[10];
int main()
{
    int n;
    int i;
    printf("Enter number of students: ");
    scanf("%d",&n);
    for(i=0;i<n;i++)
    {
        printf("Enter student USN: ");
        scanf("%d",&s1[i].usn);
        printf("Enter student name: ");
        scanf("%s",&s1[i].name);
        printf("Enter student phone number: ");
        scanf("%d",&s1[i].mobileno);
    }
    return 0;
}
